{
	if(PlayerInfo[playerid][pLogado] && PlayerInfo[playerid2][pLogado] && GetPlayerVirtualWorld(playerid) == GetPlayerVirtualWorld(playerid2))
	{
		new Float:x1,Float:y1,Float:z1;
