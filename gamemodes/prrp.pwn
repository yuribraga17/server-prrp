				    {
				        CelularData[playerid][TempoChamada][1]++;
				        CelularData[playerid][TempoChamada][0] = 0;
				    }
				    if(CelularData[playerid][TempoChamada][1] == 60)
				    {
				        CelularData[playerid][TempoChamada][2]++;
				        CelularData[playerid][TempoChamada][1] = 0;
