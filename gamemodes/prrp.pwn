		 	{
		        StopAudioStreamForPlayer(type);
				ouvindoxmradio[type] = 0;
				ouvindoxmradioID[type] = 0;
			}

			new strl[126];
			format(strl, 126, "%s mandou o jogador %s para Los Santos. [/mandarls]", PlayerName(playerid, 0), PlayerName(type, 0));
			LogCMD_A(playerid, strl);
		}
	}
	return 1;
}

