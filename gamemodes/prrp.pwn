		    if(PlayerInfo[playerid][pEntrouCasa] != -1)
		    {
				if(HouseInfo[PlayerInfo[playerid][pEntrouCasa]][hDono] == PlayerInfo[playerid][pID])
				{
                    PlayerInfo[playerid][pGrana] = PlayerInfo[playerid][pGrana]-qnt;
