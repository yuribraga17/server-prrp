    {
        if (!IsPlayerConnected(targetid)) return SendClientMessage(playerid, COLOR_WHITE, "{FF6347}Este jogador não está conectado!");
        if(PlayerInfo[playerid][pAdmin] >= 5)
        {
            new admnome[24];
