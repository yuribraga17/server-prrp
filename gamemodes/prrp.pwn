					SendClientMessage(vehowner,COLOR_LIGHTGREEN,str);
					format(str,sizeof(str)," Você tem 72 horas para pagar R$%d caso deseje recuperar seu veiculo. (( /aceitarcobranca ))",price_to_keep_car);
