forward QUERY_LOAD_MODELS();
public QUERY_LOAD_MODELS()
{
	new b[256];
	new rows,fields;
	cache_get_data(rows, fields);
	new models = rows;
 	for(new i; i < models; i++)
  	{
        cache_get_field_content(i,"model",b);  				VehicleModelInfo[i][vmModel] = strval(b);
