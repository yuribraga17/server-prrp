{
	// Return Z height value
    return (GROUND_Z_COORD2 + FloorZOffsets2[floorid]);
}

stock Float:GetDoorsZCoordForFloor2(floorid)
{
    // Return Z height value
	return (GROUND_Z_COORD2 + FloorZOffsets2[floorid]);
}

