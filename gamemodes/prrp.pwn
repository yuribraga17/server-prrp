					  	{
	       					JoinPokerTable(playerid, t);
	            		}
						else
						{
	     					return SendClientMessage(playerid, COLOR_WHITE, "{FF6347}[USE]:{FFFFFF} /sentarmesa (senha)");
	     				}
	      			}
				  	else
				  	{
	      				JoinPokerTable(playerid, t);
	      			}
