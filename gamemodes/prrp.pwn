			else return SendClientMessage(playerid,COLOR_LIGHTRED, "ERRO:{FFFFFF} Este veículo não tem um rádio instalado.");
	    }
    }
    else
    {
        new c = -1;
	    if(PlayerInfo[playerid][pEntrouCasa] != -1) c = PlayerInfo[playerid][pEntrouCasa];
