	new taperto = 0;
 	for(new i = 1; i < MAX_GARAGES; i++)
	{
		if( IsPlayerInRangeOfPoint( playerid, 10.0, tGarage[ i ][ Position ][ 0 ], tGarage[ i ][  Position ][ 1 ], tGarage[ i ][ Position ][ 2 ] ) )
