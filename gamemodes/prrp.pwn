		PontoEntrega[entid][emX],
		PontoEntrega[entid][emY],
		PontoEntrega[entid][emZ],
		PontoEntrega[entid][emEmpresa],
		PontoEntrega[entid][emStock],
		PontoEntrega[entid][emPagProd],
		PontoEntrega[entid][emStockMax],
