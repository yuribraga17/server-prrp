		static query[2048];
		format(query, sizeof(query), "UPDATE `furniture` SET `mCriado` = '%d', `mModelo` = '%d', `mX` = '%.4f', `mY` = '%.4f', `mZ` = '%.4f', `mRX` = '%.4f', `mRY` = '%.4f', `mRZ` = '%.4f', `mWorld` = '%d' WHERE `mID` = '%d'",
