    SetPVarInt(playerid, "duration", 0);
    SetPVarString(playerid, "url", "");
    SetPVarFloat(playerid, "posX", 0.0);
    SetPVarFloat(playerid, "posY", 0.0);
    SetPVarFloat(playerid, "posZ", 0.0);
    SetPVarFloat(playerid, "distance", 50.0);
