	if(sscanf(params, "uu", targetid,targetid2)) SendClientMessage(playerid, COLOR_LIGHTRED, "ERRO:{FFFFFF} /enviarjogador [Id do Player a receber o player] [Id do Player a ser enviado]");
	else
	{
