				GivePlayerMoneyCA(playerid,(EmpInfo[VendoEmpresa[playerid]][ePreco]/2));

				if(EmpInfo[VendoEmpresa[playerid]][ePreco] == 0) EmpInfo[VendoEmpresa[playerid]][ePreco] = 50000;

				new empid = VendoEmpresa[playerid];
