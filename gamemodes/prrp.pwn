{
        switch(dialogid)
        {
                case DIALOG_CGAMESCALLPOKER:
                {
                        if(GetPVarInt(playerid, "pkrChips") > 0) {
                                SetPVarInt(playerid, "pkrActionChoice", 1);

                                new tableid = GetPVarInt(playerid, "pkrTableID")-1;
