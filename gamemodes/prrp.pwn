				SendClientMessage(playerid, COLOR_CINZA, string);
				return 1;
			}
            else if(EmpInfo[PlayerInfo[playerid][pEntrouEmpresa]][eTipo] == EMP_TIPO_BANCO)
			{
				SendClientMessage(playerid, COLOR_LIGHTRED, "ERRO:{FFFFFF} /depositar [quantia]");
