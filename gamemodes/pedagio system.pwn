					    SendClientMessageEx(playerid, COLOR_YELLOW,"Você armazenou %s no %s", ReturnWeaponName(weaponid), g_arrVehicleNames[vehicleVariables[vehicle_fid][vVehicleModelID]-400]);
						return 1;
					}
