stock AnimationCheck(playerid)
{
	return (deathmode[playerid] || BitFlag_Get(g_PlayerFlags[playerid], IS_PLAYER_TAZER) || PlayerData[playerid][pJailed] || BitFlag_Get(g_PlayerFlags[playerid],PLAYER_CUFFED));
}

