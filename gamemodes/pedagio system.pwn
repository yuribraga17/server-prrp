stock SaveGraffiti(ID)
{
    new string[50];
	format(string, sizeof(string), "/Graffitis/%d.ini", ID);
	if(dini_Exists(string))
	{
		dini_Set(string,"graffcreator",graff_Info[ID][graffcreator]);
