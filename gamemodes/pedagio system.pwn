						    return 1;
						}
					    DestroyDynamicObject(CarPlace[i][weaponid][cPobj]);
						GivePlayerValidWeapon(playerid, CarData[i][carWeapon][weaponid], CarData[i][carAmmo][weaponid], CarData[i][carWeaponLicense][weaponid]);

