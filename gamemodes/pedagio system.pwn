    for (new i = 0; i != MAX_DROP_ITEMS; i ++) if (GunInfo[i][DropID] != -1 && IsPlayerInRangeOfPoint(playerid, 1.5, GunInfo[i][DropGunPosX], GunInfo[i][DropGunPosY], GunInfo[i][DropGunPosZ])) return i;

