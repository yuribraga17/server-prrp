Float:ConvertFloatToOneDP(Float:value)
{
	new Float:result;
	new tempInt;
	result = value * 10;
	tempInt = floatround(result);
	result = float(tempInt);
	result = result / 10;

	result += 0.0000001;
	
	return result;
}


