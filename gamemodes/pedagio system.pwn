stock CountVehiclePlayers(vehicle)
{
	new count;
	foreach(new i : Player) if(IsPlayerInAnyVehicle(i) && GetPlayerVehicleID(i) == vehicle && GetPlayerState(i) != PLAYER_STATE_DRIVER) count++;
	return count;
}

