stock IncreaseTime(&hh = 0, &mm = 0, &ss = 0)
{
	if(ss > 59)
	{
	    mm++;
	    ss= ss - 60;
	}

	if(mm > 59)
	{
	    hh++;
	    mm= mm - 60;
	}

	if(hh > 23)
	{
	    hh = hh - 24;
	}
	return 1;
}

