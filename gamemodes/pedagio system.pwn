					new Float:grams_size = (DrugData[PlayerDrug[playerid][slot][drugType]][DRUGDATA_TYPE]) ? PlayerDrug[playerid][slot][drugQTY] / 2.285714 : PlayerDrug[playerid][slot][drugQTY];

