                weaponid--;

				if(weaponid >= 0 && weaponid < MAX_CAR_WEAPONS && CarData[i][carWeapon][weaponid])
				{
					if (!CarPlace[i][weaponid][cPType] || !IsDoorVehicle(CarData[i][carVehicle]))
