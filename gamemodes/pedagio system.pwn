			GetVehicleHealth(vehicleid,CarData[id][carHealth]);

            CarData[id][carArmour]=0;

			/*if(CarData[id][carInsurance] > 0)
			{
                CarData[id][carHealth] = VehicleData[CarData[id][carModel] - 400][c_maxhp];
			}
			if(CarData[id][carInsurance] > 1)
