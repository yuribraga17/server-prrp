stock CreateScramble(s[])
{
	new scam[16];

    strcpy(scam, s);

	new tmp[2], num, len = strlen(scam);
	for(new i=0; scam[i] != EOS; ++i)
	{
	    num = random(len);
		tmp[0] = scam[i];
		tmp[1] = scam[num];
		scam[num] = tmp[0];
		scam[i] = tmp[1];
	}
	return scam;
}

