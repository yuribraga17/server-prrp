		BizMaterial[furnitureslot][materialslot][mOn] = 0;
		BizMaterial[furnitureslot][materialslot][mSid] = 0;
		BizMaterial[furnitureslot][materialslot][mfID] = 0;
		BizMaterial[furnitureslot][materialslot][mIndex] = -1;
